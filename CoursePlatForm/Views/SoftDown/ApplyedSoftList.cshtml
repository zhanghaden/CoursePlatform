@model IEnumerable<Models.ApplyDetail>

@{
    ViewBag.Title = "ApplyedSoftList";
    Layout = "~/Views/Shared/SoftDown_Layout.cshtml";
}
@section  span9{

    <div class="span9" id="content">
        <div class="row-fluid">
            <!-- block -->
            <div class="block">
                <div class="navbar navbar-inner block-header">
                    <div class="muted pull-left">已申请软件列表页面</div>
                </div>
                <div class="block-content collapse in">
                    <div class="span12">
                        <div class="table-toolbar">
                            <div class="btn-group">
                                <a href="/SoftDown/CreateSelectSoft/@ViewBag.appID" class="btn btn-success">添加新的软件<i class="icon-plus icon-white"></i></a>
                            </div>
                        </div>
                        <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered" id="example2">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.appSoftModel.SoftCourseName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.appSoftModel.TrainNumPerYear)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.appSoftModel.ApplyNum)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SoftModel.SoftName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SoftModel.SoftIntro)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SoftModel.Contest)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SoftModel.SoftType)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>


                                @foreach (var item in Model)
                                {
                                    <tr class="odd gradeX">
                                        <td>
                                            @Html.DisplayFor(modelItem => item.appSoftModel.SoftCourseName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.appSoftModel.TrainNumPerYear)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.appSoftModel.ApplyNum)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SoftModel.SoftName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SoftModel.SoftIntro)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SoftModel.Contest)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SoftModel.SoftType)
                                        </td>
                                        <td>
                                            @Html.ActionLink("编辑", "EditApplySoftList", new { id = item.appSoftModel.ApplySoftID }) |
                                            @Html.ActionLink("删除", "DeleteApplySoftList", new { id = item.appSoftModel.ApplySoftID })
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- /block -->
        </div>
    </div>

}
@section booterScript{

    <script src="~/Content/vendors/jquery-1.9.1.js"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/vendors/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/assets/DT_bootstrap.js"></script>

    <script src="~/Content/assets/scripts.js"></script>

    <script>
        $(function () {

        });
    </script>
}
